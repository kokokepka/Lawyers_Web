@model Lawyers_Web_App.WEB.Models.Users.UserViewModel

<div class="col-md-12 offset-md-12">
    <form asp-action="AddPhoto" enctype="multipart/form-data" method="post" data-ajax="true"
          data-ajax-update="#addphoto" data-ajax-mode="replace">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group">
            <input asp-for="Id" class="form-control" type="hidden" value="@Model.Id" />
        </div>
        <div class="form-group">
            <label asp-for="Foto" class="control-label"></label>
            <input asp-for="Foto" class="form-control-file" type="file" />
            <span asp-validation-for="Foto" class="text-danger"></span>
        </div>
        <div class="form-group">
            <input type="submit" value="Загрузить" class="btn btn-primary" />
        </div>
    </form>
</div>