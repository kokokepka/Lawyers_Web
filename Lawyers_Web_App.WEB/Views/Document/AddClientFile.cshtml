@model Lawyers_Web_App.WEB.Models.DocViewModel

<div class="col-md-12">
    <form asp-action="AddClientFile" enctype="multipart/form-data" method="post">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group">
            <label asp-for="UploadedFile" class="control-label"></label>
            <input asp-for="UploadedFile" class="form-control" type="file"/>
            <span asp-validation-for="UploadedFile" class="text-danger"></span>
        </div>
        <div class="form-group">
            <input type="submit" value="Загрузить" class="btn btn-primary" />
        </div>
    </form>
</div>