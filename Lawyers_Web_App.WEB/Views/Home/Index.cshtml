@model IEnumerable<Lawyers_Web_App.WEB.Models.DocumentViewModels>


<h2>Index</h2>

<table class="table">

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Path)
            </td>
          
        </tr>
    }
</table>
